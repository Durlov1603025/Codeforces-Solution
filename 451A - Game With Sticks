#include<stdio.h>
int main()
{
    int n,m,a;
    scanf("%d %d",&n,&m);
    if(m<n){
        if(m%2==0){
            printf("Malvika");
        }
        else
            printf("Akshat");
    }
        if(n<m){
        if(n%2==0){
            printf("Malvika");
        }
        else
            printf("Akshat");
    }
            if(n==m){
        if(n%2==0){
            printf("Malvika");
        }
        else
            printf("Akshat");
    }
    return 0;
}
