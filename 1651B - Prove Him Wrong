#include <bits/stdc++.h>
using namespace std; 

int main() {
    int t, n, j,k;
    cin>>t;
    while (t--)
    {
      cout<<endl;
      cin>>n;
      if(n>19){
          cout << "NO" << endl;
      }
      else
      {
          cout << "YES" <<endl;
          for(j = 0, k = 1; j < n ; j++ )
          {
              cout <<k << " ";
              k = k*3;
          }
      }
    }
    
    return 0;
}
